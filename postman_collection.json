{
  "info": {
    "name": "Laravel Auth API - Gestion Utilisateurs",
    "description": "Collection compl√®te pour tester l'API de gestion des utilisateurs avec JWT",
    "version": "1.0.0"
  },
  "items": [
    {
      "name": "Auth",
      "items": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "url": "http://localhost:8000/api/auth/register",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Test User\",\n  \"email\": \"test@example.com\",\n  \"password\": \"password123\",\n  \"password_confirmation\": \"password123\"\n}"
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "url": "http://localhost:8000/api/auth/login",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"starcodekh@example.com\",\n  \"password\": \"12345678\"\n}"
            }
          }
        },
        {
          "name": "Profile",
          "request": {
            "method": "GET",
            "url": "http://localhost:8000/api/auth/profile",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              }
            ]
          }
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "url": "http://localhost:8000/api/auth/logout",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              }
            ]
          }
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "url": "http://localhost:8000/api/auth/refresh",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "Users Management",
      "items": [
        {
          "name": "Get All Users (Tableau)",
          "request": {
            "method": "GET",
            "url": "http://localhost:8000/api/users?per_page=10",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              }
            ]
          }
        },
        {
          "name": "Search Users by Name",
          "request": {
            "method": "GET",
            "url": "http://localhost:8000/api/users/search?name=Star",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              }
            ]
          }
        },
        {
          "name": "Get User by ID",
          "request": {
            "method": "GET",
            "url": "http://localhost:8000/api/users/1",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              }
            ]
          }
        },
        {
          "name": "Update User",
          "request": {
            "method": "PUT",
            "url": "http://localhost:8000/api/users/1",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Name\",\n  \"email\": \"updated@example.com\"\n}"
            }
          }
        },
        {
          "name": "Change Password",
          "request": {
            "method": "POST",
            "url": "http://localhost:8000/api/users/change-password",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"current_password\": \"12345678\",\n  \"new_password\": \"newpassword123\",\n  \"new_password_confirmation\": \"newpassword123\"\n}"
            }
          }
        },
        {
          "name": "Delete User",
          "request": {
            "method": "DELETE",
            "url": "http://localhost:8000/api/users/1",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              }
            ]
          }
        },
        {
          "name": "Get Statistics",
          "request": {
            "method": "GET",
            "url": "http://localhost:8000/api/users/statistics",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer YOUR_TOKEN_HERE"
              }
            ]
          }
        }
      ]
    }
  ]
}
